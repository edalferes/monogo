# Exemplo de variáveis de ambiente para Monogo
# Copie para .env e ajuste os valores

# Aplicação
APP_NAME=monogo
APP_VERSION=1.0.0
APP_ENVIRONMENT=development
APP_PORT=8080

# Banco de dados
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_USER=postgres
DATABASE_PASSWORD=postgres
DATABASE_NAME=monogo
DATABASE_SSL_MODE=disable

# JWT
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRY_HOUR=24

# Logger
LOGGER_LEVEL=debug
LOGGER_FORMAT=console
